    <!-- Main (Home) section -->
    <section class="section" id="head">
        <div class="container">

            <div class="row">
                <div class="col-md-10 col-lg-10 col-md-offset-1 col-lg-offset-1 text-center">   

                    <h1 class="title">{{title}}</h1>
                    <h2 class="subtitle">{{description}}</h2>

                    <h3 class="tagline">
                        Doing good is part of our code!
                    </h3>

                </div>
            </div> 
        
            <div class="row">
                <div class="col-md-10 col-lg-10 col-md-offset-1 col-lg-offset-1">  
                    <div class="signupbox FormClose">
                        <div class="row">
                            <div class="col-md-12 text-center">
                                <h2>Sign up</h2>
                            </div>
                        </div>
                        <form class="form-horizontal">
                          <div class="row form-group">
                            <label class="control-label col-sm-3" for="inputSuccess3">Full name</label>
                            <div class="col-sm-9">
                              <input type="text" class="form-control" id="inputSuccess3" placeholder="Enter your full name" required />
                            </div>
                          </div>
                          <div class="row form-group">
                            <label for="inputEmail3" class="col-sm-3 control-label">Gender</label>
                            <div class="col-sm-3">
                              <div class="radio">
                                  <label>
                                    <input type="radio" name="optionsRadios" id="optionsRadios1" value="option1" checked>
                                    Male
                                  </label>
                                </div>
                            </div>
                            <div class="col-sm-3">
                                <div class="radio">
                                  <label>
                                    <input type="radio" name="optionsRadios" id="optionsRadios2" value="option2">
                                    Female
                                  </label>
                                </div>
                            </div>
                          </div>
                          <div class="row form-group">
                            <label for="inputEmail3" class="col-sm-3 control-label">Phone number</label>
                            <div class="col-sm-9">
                                <div class="input-group">
                                    <span class="input-group-addon">+91 </span>
                                    <input type="text" class="form-control" id="inputEmail3" placeholder="Enter your phone number" required />
                                </div>
                            </div>
                          </div>
                          <div class="row form-group">
                            <label for="inputEmail3" class="col-sm-3 control-label">Email</label>
                            <div class="col-sm-9">
                              <input type="email" class="form-control" id="inputEmail3" placeholder="Enter your e-mail address" required />
                            </div>
                          </div>
                          <div class="row form-group">
                            <label for="inputEmail3" class="col-sm-3 control-label">Address</label>
                            <div class="col-sm-9">
                                <textarea class="form-control" placeholder="Enter your home address" rows="3" required ></textarea>
                            </div>
                          </div>
                          <div class="row form-group">
                            <label for="inputEmail3" class="col-sm-3 control-label">City</label>
                            <div class="col-sm-9">
                              <input type="text" class="form-control" id="inputEmail3" placeholder="Enter your city" />
                            </div>
                          </div>
                          <div class="row form-group">
                            <label for="inputEmail3" class="col-sm-3 control-label">District</label>
                            <div class="col-sm-9">
                                <select class="form-control">
                                  <option>Alappuzha</option>
                                  <option>Ernakulam</option>
                                  <option>Idukki</option>
                                  <option>Kannur</option>
                                  <option>Kasaragod</option>
                                  <option>Kollam</option>
                                  <option>Kottayam</option>
                                  <option>Kozhikode</option>
                                  <option>Malappuram</option>
                                  <option>Palakkad</option>
                                  <option>Pathanamthitta</option>
                                  <option>Thiruvananthapuram</option>
                                  <option>Thrissur</option>
                                  <option>Wayanad</option>
                                </select>
                            </div>
                          </div>
                          <div class="row form-group">
                            <label for="inputEmail3" class="col-sm-3 control-label">Pin Code</label>
                            <div class="col-sm-9">
                              <input type="text" class="form-control" id="inputEmail3" placeholder="Enter your postal pincode" />
                            </div>
                          </div>
                          <div class="row form-group">
                            <label for="inputEmail3" class="col-sm-3 control-label">About yourself</label>
                            <div class="col-sm-9">
                                <textarea class="form-control" placeholder="Tell us about you and why you wish to join us?" rows="3" required ></textarea>
                            </div>
                          </div>
                          <div class="row form-group">
                            <label for="inputEmail3" class="col-sm-3 control-label">Mozillian ID</label>
                            <div class="col-sm-9">
                              <input type="text" class="form-control" id="inputEmail3" placeholder="Enter your profile link at Mozillian.org" />
                            </div>
                          </div>
                          <div class="row form-group">
                            <label for="inputEmail3" class="col-sm-3 control-label">IRC ID</label>
                            <div class="col-sm-9">
                              <input type="text" class="form-control" id="inputEmail3" placeholder="Enter your IRC username" />
                            </div>
                          </div>
                          <div class="row form-group">
                            <label for="inputEmail3" class="col-sm-3 control-label">Github ID</label>
                            <div class="col-sm-9">
                              <input type="text" class="form-control" id="inputEmail3" placeholder="Enter your github username" />
                            </div>
                          </div>
                          <div class="row form-group">
                            <div class="col-sm-offset-3 col-sm-9">
                              <div class="checkbox">
                                <label>
                                  <input type="checkbox" required /> I declare that the above informations given are true.
                                </label>
                              </div>
                            </div>
                          </div>
                          <div class="row form-group">
                            <div class="col-sm-4 col-lg-offset-2 text-center">
                              <button id="signup_submit_btn" type="submit" class="btn btn-hollow-invert">Submit</button>
                            </div>
                            <div class="col-sm-4 text-center">
                              <button id="signup_cancel_btn" onclick="return false;" class="btn btn-hollow-invert">Cancel</button>
                            </div>
                          </div>
                        </form>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-10 col-lg-10 col-md-offset-1 col-lg-offset-1 text-center">  

                    <button id="home_signup_btn" type="button" class="btn btn-hollow">Sign up</button>
        
                </div>
            </div> 
        </div>
    </section>

